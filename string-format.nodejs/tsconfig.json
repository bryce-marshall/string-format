{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "removeComments": true,
        "sourceMap": true,
        "declaration": true,
        "noImplicitAny": false,
        "outDir": "build"/*,
        "lib": [
            "es2016",
            "dom"
        ]*/
    },
    "include": [
        "*.ts",
        "package-src"
    ],
    "exclude": [
        ".vscode",
        "build",
        "node_modules"
    ]
}
